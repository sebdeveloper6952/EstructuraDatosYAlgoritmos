        xmlns:local="clr-namespace:HashTables"
        mc:Ignorable="d"
        Title="HashTables" Height="256.667" Width="525" Loaded="Window_Loaded">
    <Grid>
        <GroupBox x:Name="gB_Ingresar" Header="Ingresar Empleado" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="199" Width="209">
