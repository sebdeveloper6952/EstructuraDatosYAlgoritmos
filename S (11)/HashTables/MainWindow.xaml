                <Button x:Name="btn_Eliminar" Content="Eliminar" HorizontalAlignment="Left" Margin="139,51,0,0" VerticalAlignment="Top" Width="75" Click="btn_Eliminar_Click"/>
                <Label x:Name="l_Resultado" Content="Resultado" HorizontalAlignment="Left" Margin="87,87,0,0" VerticalAlignment="Top"/>
                <Label x:Name="l_NombreEmpleado" Content="" HorizontalAlignment="Left" Margin="10,122,0,0" VerticalAlignment="Top" Width="215"/>
